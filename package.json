    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
